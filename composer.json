{
    "name": "get-things-done/package-skeleton-laravel",
    "description": "This is my package package-skeleton-laravel",
    "keywords": [
        "get-things-done",
        "laravel",
        "package-skeleton-laravel"
    ],
    "homepage": "https://github.com/get-things-done/package-skeleton-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Cao Minh Duc",
            "email": "caominhduc31081@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "GetThingsDone\\PackageSkeletonLaravel\\": "src",
            "GetThingsDone\\PackageSkeletonLaravel\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GetThingsDone\\PackageSkeletonLaravel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": [
            "@putenv XDEBUG_MODE=coverage",
            "vendor/bin/phpunit --coverage-html coverage"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "GetThingsDone\\PackageSkeletonLaravel\\PackageSkeletonLaravelServiceProvider"
            ],
            "aliases": {
                "PackageSkeletonLaravel": "GetThingsDone\\PackageSkeletonLaravel\\PackageSkeletonLaravelFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
